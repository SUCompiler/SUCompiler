{ 
    i = 3;  j = 4;

    IF (i == j) {
    	t = 200;
    }
    ELSE {
    	f = -200;
    };

    IF (i < j) { 
    	t = 300; 
    };

    IF      (i == 1) { f = 10; }
    ELSE IF (i == 2) { f = 20; }
    ELSE IF (i == 3) { t = 30; }
    ELSE IF (i == 4) { f = 40; }
    ELSE            { f = -1; };

    
    IF (i == 3) { 
    	IF j == 2 { 
    		t = 500 
    	} ELSE {
    	f = -500;
    	}
    }
}
